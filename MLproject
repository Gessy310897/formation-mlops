name: hospital-readmission-xgb

entry_points:
  main:
    parameters:
      remote_server_uri: {type: str, default: https://user-gessynk-mlflow.user.lab.sspcloud.fr}
      experiment_name: {type: str, default: hospital-readmission-xgb}
      run_name: {type: str, default: test-xgb}
      max_depth: {type: int, default: 3}
      learning_rate: {type: float, default: 0.1}
    command: >
      python src/train.py {remote_server_uri} {experiment_name} {run_name} {max_depth} {learning_rate}
